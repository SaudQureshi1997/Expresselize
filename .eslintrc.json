{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "standard",
        "import"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [],
    "rules": {
        "semi": "off",
        "quotes":"off",
        "comma-dangle": "off"
    }
}